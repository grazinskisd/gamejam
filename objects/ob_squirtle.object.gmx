        attack = true;
    }else{
        attack = false;
    }
}
// die
if(life &lt;= 0){
    rand_sound = irandom(6);
    switch(rand_sound){
        case(1):
            audio_play_sound(snd_blood_1, 1, 0);
            break;
        case(2):
            audio_play_sound(snd_blood_2, 1, 0);
            break;
