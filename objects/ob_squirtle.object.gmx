            <string>//Move if rembo is in particular distance

if(instance_exists(ob_rembo)){
    t_x = ob_rembo.x;   //target x
    t_y = ob_rembo.y;   //target y
    distance = sqrt(sqr(t_x-x) + sqr(t_y-y));
    
    move = false;
    attack = false;
    
    //update cooldown
    if(cooldown &gt; 0){
        cooldown--;
    }
    
    //follow rembo
    if(distance &lt;= 256){
        move = true;
    }else{
        move = false;
